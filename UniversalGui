local hrp = chr:WaitForChild("HumanoidRootPart")
local mouse = player:GetMouse()
player.CharacterAdded:Connect(function(char)
	chr = char
	hmd = char:WaitForChild("Humanoid")
	hrp = char:WaitForChild("HumanoidRootPart")
end)
